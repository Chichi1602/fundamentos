#include <iostream>

using namespace std;

int main()
{
    float a,b, suma, resta, mult, divi;
    cout<<"Ingresar el primer valor ";
    cin>>a;
    cout<<"ingresar el segundo valor ";
    cin>>b;
    
    suma=a+b;
    cout<<"La suma es: "<<suma<<endl;
    
    resta=a-b;
    cout<<"La resta es: "<<resta<<endl;
    
    mult=a*b;
    cout<<"La mutiplicacion es: "<<mult<<endl;
    
    divi=a/b;
    cout<<"La division es: "<<divi<<endl;
    
    return 0;
}
