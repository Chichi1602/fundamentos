#include <iostream>
using namespace std;

int main()
{
    int n,acum=1 ;
    do {

        cout << "Ingresar numero de elementos: ";
        cin >> n;
    } while (n < 0);
    
    
    for (int i = 1; i < n; i++) {
    
        acum = acum * i;
       }
    cout << "el factorial es: " << acum;
    return 0;

}
