#include <iostream>

using namespace std;

int main()
{
    
    float v1,v2,promedio;
    cout<<"Ingrese el primer valor: ";
    cin >>v1;
    cout<<"Ingrese el segundo valor: ";
    cin>>v2;
    promedio=v1+v2;
    
    cout<<"Ingrese el tercer valor: ";
    cin >>v1;
    cout<<"Ingrese el cuarto valor: ";
    cin>>v2;
    promedio=promedio+v1+v2;
    
    cout<<"Ingrese el quinto valor: ";
    cin >>v1;
    
    promedio=promedio+v1;
    promedio=promedio/5;
    
    cout<<"El promedio total es: "<<promedio;
    

    return 0;
}
